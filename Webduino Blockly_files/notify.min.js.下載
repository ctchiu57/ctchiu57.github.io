!function(o,n){n.notify={init:function(){axios.get("/blockly/msg/updateLog/"+n.LANG+".json").then(function(o){return o.data}).catch(function(o){return console.error(o),[]}).then(function(o){var n=$("#notification"),c=$("#note-check i");(function(o){if(!o.length)return!1;var n=o[0].date;return Cookies.get("notificationWindow")!==n})(o)&&(function(o){var n=$(".record");o.forEach(function(o){n.append("<h5>"+o.date+"</h5>"),o.content.forEach(function(o){n.append("<i> ("+o.type+") "+o.text+"</i>")})})}(o),Cookies.remove("notificationWindow"),n.addClass("notification-open"),c.addClass("icon-checkbox-unchecked").removeClass("icon-checkbox-checked"),function(o){var n=$("#notification"),c=$("#note-close-btn"),e=$("#note-check"),i=$("#note-check i");c.on("click",function(){n.fadeOut(200,function(){n.css({display:""}).removeClass("notification-open")})}),e.on("click",function(){i.hasClass("icon-checkbox-unchecked")?(i.removeClass("icon-checkbox-unchecked").addClass("icon-checkbox-checked"),Cookies.set("notificationWindow",o,{expires:365})):(i.addClass("icon-checkbox-unchecked").removeClass("icon-checkbox-checked"),Cookies.remove("notificationWindow"))})}(o[0].date))})}}}(window,window.Code);